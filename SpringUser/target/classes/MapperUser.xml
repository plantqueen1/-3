<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--namespace绑定一个Mapper/User接口-->
<!--接口实现类-->
<mapper namespace="luo.pojo.MapperUser">
    <select id="getSelect" resultType="User">
        select *
        from `account`
<where>
    <if test="id1 !=null">
        id1=#{id1}
    </if>
</where>
    </select>
</mapper>